<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_blue"
    tools:context="mg.studio.game.animals.Record">


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/font_one"
        android:text="RECORD"
        android:textColor="#FFFFFF"
        android:textSize="50dp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="33dp" />

    <TextView
        android:id="@+id/t1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="23dp"
        android:text="0"
        android:textColor="#FFFFFF"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/t1"
        android:layout_alignStart="@+id/t1"
        android:layout_below="@+id/t1"
        android:layout_marginTop="43dp"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:text="0" />

    <TextView
        android:id="@+id/t3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/t2"
        android:layout_alignStart="@+id/t2"
        android:layout_below="@+id/t2"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_marginTop="43dp"
        android:text="0" />

    <TextView
        android:id="@+id/t4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/t3"
        android:layout_alignStart="@+id/t3"
        android:layout_below="@+id/t3"
        android:layout_marginTop="47dp"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:text="0" />

    <TextView
        android:id="@+id/t5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/t4"
        android:layout_alignStart="@+id/t4"
        android:layout_below="@+id/t4"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_marginTop="53dp"
        android:text="0" />

    <TextView
        android:id="@+id/t6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/t1"
        android:layout_alignBottom="@+id/t1"
        android:layout_alignEnd="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:text="0"
        android:textColor="#FFFFFF"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/t7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/t3"
        android:layout_alignLeft="@+id/t6"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignStart="@+id/t6"
        android:text="0" />

    <TextView
        android:id="@+id/t8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/t4"
        android:layout_alignLeft="@+id/t7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignStart="@+id/t7"
        android:text="0" />

    <TextView
        android:id="@+id/t9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/t5"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignLeft="@+id/t8"
        android:layout_alignStart="@+id/t8"
        android:text="0" />

    <TextView
        android:id="@+id/t10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignLeft="@+id/t9"
        android:layout_alignStart="@+id/t9"
        android:layout_alignTop="@+id/t5"
        android:text="0" />

    <Button
        android:id="@+id/rt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="22dp"
        android:text="Return" />


</RelativeLayout>
